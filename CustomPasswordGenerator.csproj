<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows10.0.22000.0</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>padlock.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="check.png" />
    <None Remove="close %281%29.png" />
    <None Remove="close.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="check.png" />
    <Content Include="close (1).png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="close.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="padlock.ico">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
